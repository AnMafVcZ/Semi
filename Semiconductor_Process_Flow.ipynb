    "            'Polysilicon': {'color': (105, 105, 105), 'range': 35},  # Dark gray\n",
    "            'Copper': {'color': (184, 115, 51), 'range': 30},        # Copper\n",
    "            'Titanium': {'color': (135, 206, 235), 'range': 40},     # Sky blue\n",
    "        }\n",
    "    \n",
    "    def identify_material(self, rgb_color):\n",
    "        \"\"\"\n",
    "        Identify material based on RGB color value using Euclidean distance\n",
    "        \n",
    "        Args:\n",
    "            rgb_color: Tuple of (R, G, B) values\n",
    "            \n",
    "        Returns:\n",
    "            material_name: Best matching material\n",
    "            confidence: Distance score (lower = better match)\n",
