   "source": [
    "\n",
    "\n",
    "class EnhancedScaleMatrix:\n",
    "    \"\"\"\n",
    "    Multi-scale detection and conversion system\n",
    "    \n",
    "    Capabilities:\n",
    "    - Automatic detection of multiple scale regions\n",
    "    - OCR-based scale text recognition\n",
    "    - Region-aware scale conversion\n",
    "    - Scale bar association with text labels\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        # Conversion factors to meters\n",
    "        self.scale_units = {\n",
    "            'nm': 1e-9,   # nanometer\n",
    "            'um': 1e-6,   # micrometer  \n",
    "            'μm': 1e-6,   # micrometer (unicode)\n",
    "            'mm': 1e-3,   # millimeter\n",
    "            'cm': 1e-2,   # centimeter\n",
    "            'm': 1,       # meter\n",
    "            'pm': 1e-12,  # picometer\n",
    "            'fm': 1e-15   # femtometer\n",
    "        }\n",
    "        \n",
    "        # Scale regions with their calibrations\n",
    "        self.scale_regions = []\n",
    "        self.detected_scales = []\n",
    "        self.image_zones = []  # Different visual zones in the image\n",
    "        \n",
    "    def detect_image_zones(self, image):\n",
    "        \"\"\"\n",
    "        Detect distinct visual zones that might have different scales\n",
    "        Uses edge density, texture analysis, and contrast changes\n",
    "        \"\"\"\n",
    "        gray = cv2.cvtColor(image, cv2.COLOR_RGB2GRAY)\n",
    "        \n",
    "        # Method 1: Edge density analysis\n",
    "        edges = cv2.Canny(gray, 50, 150)\n",
    "        \n",
    "        # Divide image into grid and analyze edge density\n",
    "        h, w = gray.shape\n",
    "        grid_size = 100  # 100x100 pixel blocks\n",
    "        zones = []\n",
    "        \n",
    "        for y in range(0, h - grid_size, grid_size):\n",
    "            for x in range(0, w - grid_size, grid_size):\n",
    "                region = edges[y:y+grid_size, x:x+grid_size]\n",
    "                edge_density = np.sum(region) / (grid_size * grid_size)\n",
    "                \n",
    "                # High edge density might indicate fine-scale features\n",
    "                if edge_density > 0.1:  # Threshold for \"detailed\" regions\n",
    "                    zones.append({\n",
    "                        'bbox': (x, y, grid_size, grid_size),\n",
    "                        'type': 'high_detail',\n",
    "                        'edge_density': edge_density\n",
    "                    })\n",
    "        \n",
    "        # Method 2: Detect image boundaries/frames\n",
    "        contours, _ = cv2.findContours(edges, cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)\n",
    "        \n",
    "        for contour in contours:\n",
    "            x, y, w, h = cv2.boundingRect(contour)\n",
    "            # Look for rectangular regions that might be insets\n",
    "            if w > 200 and h > 200 and w * h > 50000:  # Significant size\n",
    "                zones.append({\n",
    "                    'bbox': (x, y, w, h),\n",
    "                    'type': 'bounded_region',\n",
    "                    'contour': contour\n",
    "                })\n",
    "        \n",
    "        self.image_zones = zones\n",
    "        return zones\n",
    "    \n",
    "    def detect_scale_bars_and_text(self, image):\n",
    "        \"\"\"\n",
    "        Comprehensive scale detection combining bar detection and OCR\n",
    "        \"\"\"\n",
    "        gray = cv2.cvtColor(image, cv2.COLOR_RGB2GRAY)\n",
    "        h, w = gray.shape\n",
    "        \n",
    "        # Step 1: Detect scale bars (horizontal lines)\n",
    "        scale_bars = self._detect_scale_bars(image)\n",
    "        \n",
    "        # Step 2: OCR to find scale text\n",
    "        scale_texts = self._detect_scale_text(image)\n",
    "        \n",
    "        # Step 3: Associate scale bars with nearby text\n",
    "        scale_associations = self._associate_bars_with_text(scale_bars, scale_texts)\n",
    "        \n",
    "        # Step 4: Create scale regions\n",
    "        self._create_scale_regions(scale_associations, image.shape)\n",
    "        \n",
    "        return scale_associations\n",
    "    \n",
    "    def _detect_scale_bars(self, image):\n",
    "        \"\"\"Enhanced scale bar detection\"\"\"\n",
    "        gray = cv2.cvtColor(image, cv2.COLOR_RGB2GRAY)\n",
    "        \n",
    "        # Multiple kernel sizes for different scale bar sizes\n",
    "        scale_bars = []\n",
    "        \n",
    "        for kernel_width in [15, 25, 40, 60]:\n",
    "            # Horizontal line detection\n",
    "            horizontal_kernel = cv2.getStructuringElement(cv2.MORPH_RECT, (kernel_width, 1))\n",
    "            horizontal_lines = cv2.morphologyEx(gray, cv2.MORPH_OPEN, horizontal_kernel)\n",
    "            \n",
    "            # Find contours\n",
    "            contours, _ = cv2.findContours(horizontal_lines, cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)\n",
    "            \n",
    "            for contour in contours:\n",
    "                x, y, w, h = cv2.boundingRect(contour)\n",
    "                \n",
    "                # Filter criteria for scale bars\n",
    "                aspect_ratio = w / h if h > 0 else 0\n",
    "                \n",
    "                if (w > 30 and h < 15 and aspect_ratio > 5 and \n",
    "                    w < gray.shape[1] * 0.3):  # Not too wide (likely not image border)\n",
    "                    \n",
    "                    scale_bars.append({\n",
    "                        'bbox': (x, y, w, h),\n",
    "                        'center': (x + w//2, y + h//2),\n",
    "                        'length_pixels': w,\n",
    "                        'confidence': min(aspect_ratio / 10, 1.0)\n",
    "                    })\n",
    "        \n",
    "        # Remove duplicates (bars detected by multiple kernels)\n",
    "        scale_bars = self._remove_duplicate_bars(scale_bars)\n",
    "        \n",
    "        return scale_bars\n",
    "    \n",
    "    def _detect_scale_text(self, image):\n",
    "        \"\"\"\n",
    "        OCR-based detection of scale text (e.g., \"100 nm\", \"5 μm\")\n",
    "        \"\"\"\n",
    "        # Preprocess for better OCR\n",
    "        gray = cv2.cvtColor(image, cv2.COLOR_RGB2GRAY)\n",
    "        \n",
    "        # Try multiple preprocessing approaches\n",
    "        preprocessed_images = [\n",
    "            gray,  # Original\n",
    "            cv2.threshold(gray, 0, 255, cv2.THRESH_BINARY + cv2.THRESH_OTSU)[1],  # Binary\n",
    "            cv2.adaptiveThreshold(gray, 255, cv2.ADAPTIVE_THRESH_GAUSSIAN_C, cv2.THRESH_BINARY, 11, 2)  # Adaptive\n",
    "        ]\n",
    "        \n",
    "        scale_texts = []\n",
    "        \n",
    "        for proc_img in preprocessed_images:\n",
    "            # OCR with specific config for scale text\n",
    "            config = '--oem 3 --psm 6 -c tessedit_char_whitelist=0123456789.,μumnmcmMN '\n",
    "            \n",
    "            try:\n",
    "                ocr_data = pytesseract.image_to_data(proc_img, config=config, output_type=pytesseract.Output.DICT)\n",
    "                \n",
    "                for i in range(len(ocr_data['text'])):\n",
    "                    text = ocr_data['text'][i].strip()\n",
    "                    \n",
    "                    if self._is_scale_text(text):\n",
    "                        x, y, w, h = (ocr_data['left'][i], ocr_data['top'][i], \n",
    "                                    ocr_data['width'][i], ocr_data['height'][i])\n",
    "                        \n",
    "                        # Extract number and unit\n",
    "                        number, unit = self._parse_scale_text(text)\n",
    "                        \n",
    "                        if number and unit:\n",
    "                            scale_texts.append({\n",
    "                                'text': text,\n",
    "                                'bbox': (x, y, w, h),\n",
    "                                'center': (x + w//2, y + h//2),\n",
    "                                'number': number,\n",
    "                                'unit': unit,\n",
    "                                'confidence': ocr_data['conf'][i]\n",
    "                            })\n",
    "            except:\n",
    "                continue  # Skip if OCR fails\n",
    "        \n",
    "        # Remove duplicates and low confidence detections\n",
    "        scale_texts = [st for st in scale_texts if st['confidence'] > 30]\n",
    "        scale_texts = self._remove_duplicate_texts(scale_texts)\n",
    "        \n",
    "        return scale_texts\n",
    "    \n",
    "    def _is_scale_text(self, text):\n",
    "        \"\"\"Check if text looks like a scale annotation\"\"\"\n",
    "        # Patterns for scale text: \"100 nm\", \"5μm\", \"2.5 mm\", etc.\n",
    "        scale_pattern = r'\\d+\\.?\\d*\\s*(nm|um|μm|mm|cm|m|pm|fm)'\n",
    "        return bool(re.search(scale_pattern, text, re.IGNORECASE))\n",
    "    \n",
    "    def _parse_scale_text(self, text):\n",
    "        \"\"\"Extract number and unit from scale text\"\"\"\n",
    "        # Pattern to extract number and unit\n",
    "        pattern = r'(\\d+\\.?\\d*)\\s*(nm|um|μm|mm|cm|m|pm|fm)'\n",
    "        match = re.search(pattern, text, re.IGNORECASE)\n",
    "        \n",
    "        if match:\n",
    "            number = float(match.group(1))\n",
    "            unit = match.group(2).lower()\n",
    "            # Normalize unit\n",
    "            if unit == 'um':\n",
    "                unit = 'μm'\n",
    "            return number, unit\n",
    "        \n",
    "        return None, None\n",
    "    \n",
    "    def _associate_bars_with_text(self, scale_bars, scale_texts):\n",
    "        \"\"\"\n",
    "        Associate scale bars with nearby scale text\n",
    "        \"\"\"\n",
    "        associations = []\n",
    "        used_texts = set()\n",
    "        \n",
    "        for bar in scale_bars:\n",
    "            bar_center = bar['center']\n",
    "            best_text = None\n",
    "            min_distance = float('inf')\n",
    "            \n",
    "            # Find closest scale text within reasonable distance\n",
    "            for i, text in enumerate(scale_texts):\n",
    "                if i in used_texts:\n",
    "                    continue\n",
    "                    \n",
    "                text_center = text['center']\n",
    "                distance = np.sqrt((bar_center[0] - text_center[0])**2 + \n",
    "                                 (bar_center[1] - text_center[1])**2)\n",
    "                \n",
    "                # Text should be reasonably close to scale bar\n",
    "                if distance < 100 and distance < min_distance:  # 100 pixel max distance\n",
    "                    min_distance = distance\n",
    "                    best_text = (i, text)\n",
    "            \n",
    "            if best_text:\n",
    "                text_idx, text_data = best_text\n",
    "                used_texts.add(text_idx)\n",
    "                \n",
    "                # Calculate pixels per unit\n",
    "                pixels_per_unit = bar['length_pixels'] / text_data['number']\n",
    "                \n",
    "                associations.append({\n",
    "                    'scale_bar': bar,\n",
    "                    'scale_text': text_data,\n",
    "                    'pixels_per_unit': pixels_per_unit,\n",
    "                    'unit': text_data['unit'],\n",
    "                    'real_distance': text_data['number'],\n",
    "                    'confidence': (bar['confidence'] + text_data['confidence']/100) / 2\n",
    "                })\n",
    "        \n",
    "        return associations\n",
    "    \n",
    "    def _create_scale_regions(self, associations, image_shape):\n",
    "        \"\"\"\n",
    "        Create scale regions based on detected scales and image zones\n",
    "        \"\"\"\n",
    "        h, w = image_shape[:2]\n",
    "        self.scale_regions = []\n",
    "        \n",
    "        for assoc in associations:\n",
    "            bar_center = assoc['scale_bar']['center']\n",
    "            \n",
    "            # Determine region of influence for this scale\n",
    "            # Simple approach: quadrant-based regions\n",
    "            region_w = w // 2\n",
    "            region_h = h // 2\n",
    "            \n",
    "            if bar_center[0] < w // 2:  # Left side\n",
    "                region_x = 0\n",
    "            else:  # Right side\n",
    "                region_x = w // 2\n",
    "                \n",
    "            if bar_center[1] < h // 2:  # Top\n",
    "                region_y = 0\n",
    "            else:  # Bottom\n",
    "                region_y = h // 2\n",
    "            \n",
    "            # More sophisticated: use detected image zones\n",
    "            best_zone = None\n",
    "            for zone in self.image_zones:\n",
    "                zx, zy, zw, zh = zone['bbox']\n",
    "                if (zx <= bar_center[0] <= zx + zw and \n",
    "                    zy <= bar_center[1] <= zy + zh):\n",
    "                    best_zone = zone\n",
    "                    break\n",
    "            \n",
    "            if best_zone:\n",
    "                region_bbox = best_zone['bbox']\n",
    "            else:\n",
    "                region_bbox = (region_x, region_y, region_w, region_h)\n",
    "            \n",
    "            self.scale_regions.append({\n",
    "                'bbox': region_bbox,\n",
    "                'pixels_per_unit': assoc['pixels_per_unit'],\n",
    "                'unit': assoc['unit'],\n",
    "                'real_distance': assoc['real_distance'],\n",
    "                'confidence': assoc['confidence'],\n",
    "                'scale_bar_location': bar_center\n",
    "            })\n",
    "    \n",
    "    def get_scale_at_position(self, x, y):\n",
    "        \"\"\"\n",
    "        Get the appropriate scale calibration for a given pixel position\n",
    "        \"\"\"\n",
    "        best_region = None\n",
    "        best_confidence = 0\n",
    "        \n",
    "        for region in self.scale_regions:\n",
    "            rx, ry, rw, rh = region['bbox']\n",
    "            \n",
    "            # Check if point is in region\n",
    "            if rx <= x <= rx + rw and ry <= y <= ry + rh:\n",
    "                if region['confidence'] > best_confidence:\n",
    "                    best_region = region\n",
    "                    best_confidence = region['confidence']\n",
    "        \n",
    "        if best_region:\n",
    "            return best_region['pixels_per_unit'], best_region['unit']\n",
    "        \n",
    "        # Fallback: use closest scale region\n",
    "        min_distance = float('inf')\n",
    "        closest_region = None\n",
    "        \n",
    "        for region in self.scale_regions:\n",
    "            bar_center = region['scale_bar_location']\n",
    "            distance = np.sqrt((x - bar_center[0])**2 + (y - bar_center[1])**2)\n",
    "            \n",
    "            if distance < min_distance:\n",
    "                min_distance = distance\n",
    "                closest_region = region\n",
    "        \n",
    "        if closest_region:\n",
    "            return closest_region['pixels_per_unit'], closest_region['unit']\n",
    "        \n",
    "        return None, None\n",
    "    \n",
    "    def convert_pixels_to_real(self, pixels, x, y, target_unit='nm'):\n",
    "        \"\"\"\n",
    "        Position-aware pixel to real-world conversion\n",
    "        \"\"\"\n",
    "        pixels_per_unit, current_unit = self.get_scale_at_position(x, y)\n",
    "        \n",
    "        if not pixels_per_unit or not current_unit:\n",
    "            return pixels, 'pixels'  # No scale available\n",
    "        \n",
    "        # Convert to meters first\n",
    "        real_meters = (pixels / pixels_per_unit) * self.scale_units[current_unit]\n",
    "        \n",
    "        # Convert to target unit\n",
    "        real_value = real_meters / self.scale_units[target_unit]\n",
    "        \n",
    "        return real_value, target_unit\n",
    "    \n",
    "    def _remove_duplicate_bars(self, scale_bars):\n",
    "        \"\"\"Remove duplicate scale bar detections\"\"\"\n",
    "        if not scale_bars:\n",
    "            return []\n",
    "        \n",
    "        # Sort by confidence\n",
    "        scale_bars.sort(key=lambda x: x['confidence'], reverse=True)\n",
    "        \n",
    "        unique_bars = []\n",
    "        for bar in scale_bars:\n",
    "            is_duplicate = False\n",
    "            for existing in unique_bars:\n",
    "                # Check if bars overlap significantly\n",
    "                overlap = self._calculate_overlap(bar['bbox'], existing['bbox'])\n",
    "                if overlap > 0.5:  # 50% overlap threshold\n",
    "                    is_duplicate = True\n",
    "                    break\n",
    "            \n",
    "            if not is_duplicate:\n",
    "                unique_bars.append(bar)\n",
    "        \n",
    "        return unique_bars\n",
    "    \n",
    "    def _remove_duplicate_texts(self, scale_texts):\n",
    "        \"\"\"Remove duplicate text detections\"\"\"\n",
    "        if not scale_texts:\n",
    "            return []\n",
    "        \n",
    "        unique_texts = []\n",
    "        for text in scale_texts:\n",
    "            is_duplicate = False\n",
    "            for existing in unique_texts:\n",
    "                # Check if texts are very close and similar\n",
    "                distance = np.sqrt((text['center'][0] - existing['center'][0])**2 + \n",
    "                                 (text['center'][1] - existing['center'][1])**2)\n",
    "                if distance < 20 and text['text'].lower() == existing['text'].lower():\n",
    "                    is_duplicate = True\n",
    "                    break\n",
    "            \n",
    "            if not is_duplicate:\n",
    "                unique_texts.append(text)\n",
    "        \n",
    "        return unique_texts\n",
    "    \n",
    "    def _calculate_overlap(self, bbox1, bbox2):\n",
    "        \"\"\"Calculate overlap ratio between two bounding boxes\"\"\"\n",
    "        x1, y1, w1, h1 = bbox1\n",
    "        x2, y2, w2, h2 = bbox2\n",
    "        \n",
    "        # Calculate intersection\n",
    "        left = max(x1, x2)\n",
    "        top = max(y1, y2)\n",
    "        right = min(x1 + w1, x2 + w2)\n",
    "        bottom = min(y1 + h1, y2 + h2)\n",
    "        \n",
    "        if left < right and top < bottom:\n",
    "            intersection = (right - left) * (bottom - top)\n",
    "            union = w1 * h1 + w2 * h2 - intersection\n",
    "            return intersection / union if union > 0 else 0\n",
    "        \n",
    "        return 0\n",
    "    \n",
    "    def analyze_image(self, image):\n",
    "        \"\"\"\n",
    "        Complete analysis pipeline for multi-scale image\n",
    "        \"\"\"\n",
    "        print(\"🔍 Analyzing multi-scale image...\")\n",
    "        \n",
    "        # Step 1: Detect visual zones\n",
    "        zones = self.detect_image_zones(image)\n",
    "        print(f\"  - Found {len(zones)} distinct image zones\")\n",
    "        \n",
    "        # Step 2: Detect scales\n",
    "        associations = self.detect_scale_bars_and_text(image)\n",
    "        print(f\"  - Detected {len(associations)} scale calibrations\")\n",
    "        \n",
    "        # Step 3: Report findings\n",
    "        for i, assoc in enumerate(associations):\n",
    "            bar = assoc['scale_bar']\n",
    "            text = assoc['scale_text']\n",
    "            print(f\"  - Scale {i+1}: {bar['length_pixels']} pixels = {text['number']} {text['unit']}\")\n",
    "            print(f\"    → {assoc['pixels_per_unit']:.2f} pixels per {text['unit']}\")\n",
    "            print(f\"    → Confidence: {assoc['confidence']:.2f}\")\n",
    "        \n",
    "        return {\n",
    "            'zones': zones,\n",
    "            'scale_associations': associations,\n",
    "            'scale_regions': self.scale_regions\n",
    "        }\n",
    "\n",
